{
  "name": "napshort-v2",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/server.ts' -e ts",
    "start": "node dist/server.js",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\" --quiet --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pedrol2b/napshort-v2.git"
  },
  "author": "Pedro Bueno <pedrolbb.e@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/pedrol2b/napshort-v2",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.13.0",
    "@types/nodemailer": "^6.4.7",
    "@types/randomcolor": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.5.0",
    "helmet": "^6.0.1",
    "jimp": "^0.22.4",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.1",
    "nodemailer": "^6.9.1",
    "nodemailer-pug-engine": "^2.0.1",
    "pug": "^3.0.2",
    "randomcolor": "^0.6.2"
  }
}
